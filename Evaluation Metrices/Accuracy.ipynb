    "def accuracy(gt,mask):\n",
    "  \n",
    "  img = cv2.bitwise_xor(gt,mask)\n",
    "  n = np.count_nonzero(img)\n",
    "  sum = mask.size\n",
    "  k = sum-n\n",
    "  acc = k/sum\n",
    "  #print(k)\n",
    "  print(acc)\n",
    "  return(acc)\n",
